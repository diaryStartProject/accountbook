{
  "extends": [
      "airbnb-base",
      "airbnb-typescript/base",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
  "parserOptions": {
    "project": ["**/tsconfig.json"]
  },
  "rules": {
    "no-console": "off",
    "no-shadow":"off",
    "@typescript-eslint/no-shadow": ["off"],
    "@typescript-eslint/no-unused-vars":["off"]
  }
}